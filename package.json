{
  "name": "neuronova",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:fast": "node quick-fix.js",
    "dev:clean": "rimraf .next && npm run dev",
    "dev:turbo": "cross-env NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next dev --turbo",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
    "dev:backend": "nodemon server/simple-backend.js",
    "test:backend": "nodemon --exec ts-node --project server/tsconfig.json server/test-server.ts",
    "build": "cross-env NODE_ENV=production next build",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "build:production": "cross-env NODE_ENV=production next build && next-sitemap",
    "build:backend": "tsc --project server/tsconfig.json",
    "start": "node backend-final-working.js",
    "start:production": "cross-env NODE_ENV=production next start -p 3000",
    "start:backend": "ts-node --project server/tsconfig.json server/app.ts",
    "start:backend:prod": "node server/dist/app.js",
    "seed": "ts-node --project server/tsconfig.json server/scripts/seed.ts",
    "setup:db": "ts-node --project server/tsconfig.json server/scripts/setup-database.ts",
    "seed:users": "ts-node --project server/tsconfig.json server/scripts/seed-users.ts",
    "seed:users-extended": "ts-node --project server/tsconfig.json server/scripts/seed-users-extended.ts",
    "seed:research": "ts-node --project server/tsconfig.json server/scripts/seed-research-simple.ts",
    "seed:research-extended": "ts-node --project server/tsconfig.json server/scripts/seed-research-extended.ts",
    "seed:research-expanded": "ts-node --project server/tsconfig.json server/scripts/seed-research-expanded.ts",
    "seed:research-week1-batch2": "ts-node --project server/tsconfig.json server/scripts/seed-research-week1-batch2.ts",
    "seed:research-week1-final": "ts-node --project server/tsconfig.json server/scripts/seed-research-week1-final.ts",
    "seed:all": "npm run setup:db && npm run seed:users && npm run seed:research",
    "seed:week1": "npm run setup:db && npm run seed:users && npm run seed:research && npm run seed:research-expanded && npm run seed:research-week1-batch2 && npm run seed:research-week1-final",
    "seed:production": "npm run setup:db && npm run seed:users-extended && npm run seed:research-extended",
    "seed:production-complete": "ts-node --project server/tsconfig.json server/scripts/seed-production-complete.ts",
    "seed:production-users": "ts-node --project server/tsconfig.json server/scripts/seed-production-users.ts",
    "seed:production-research": "ts-node --project server/tsconfig.json server/scripts/seed-production-research.ts",
    "setup:search": "ts-node --project server/tsconfig.json server/scripts/setup-search-indexes.ts",
    "setup:complete": "npm run setup:db && npm run setup:search && npm run seed:all",
    "test:db": "npm run setup:db",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:backend": "tsc --project server/tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build && npx @next/bundle-analyzer .next",
    "bundle-analyzer": "npx @next/bundle-analyzer .next",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html --chrome-flags=\"--headless\"",
    "perf": "npm run build && npm run lighthouse",
    "perf:full": "npm run build:analyze && npm run lighthouse",
    "docker:build": "docker build -t neuronova .",
    "docker:run": "docker run -p 3000:3000 neuronova",
    "export": "next export",
    "clean": "rimraf .next out node_modules/.cache server/dist",
    "clean:full": "rimraf .next out node_modules/.cache server/dist node_modules package-lock.json",
    "preinstall": "npx only-allow npm",
    "postinstall": "patch-package || true"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.10.0",
    "@heroicons/react": "^2.2.0",
    "@huggingface/inference": "^4.0.6",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^1.4.13",
    "@types/qrcode": "^1.5.5",
    "@types/recharts": "^1.8.29",
    "@types/socket.io": "^3.0.1",
    "@types/speakeasy": "^2.0.10",
    "@types/three": "^0.177.0",
    "@uploadthing/react": "^7.3.2",
    "argon2": "^0.43.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "framer-motion": "^12.19.1",
    "fuse.js": "^7.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.516.0",
    "marked": "^15.0.12",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.0",
    "multer": "^2.0.1",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.8.0",
    "openai": "^5.7.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-redux": "^9.2.0",
    "recharts": "^2.15.3",
    "redis": "^5.5.6",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^2.5.5",
    "three": "^0.177.0",
    "uploadthing": "^7.7.3",
    "validator": "^13.15.15",
    "web-vitals": "^4.2.4",
    "xml2js": "^0.6.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "^13.15.2",
    "@types/xml2js": "^0.4.14",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.2.1",
    "lint-staged": "^15.5.2",
    "next-sitemap": "^4.2.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
